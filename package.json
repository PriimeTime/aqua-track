{
  "name": "Drinky",
  "version": "1.6.4",
  "scripts": {
    "start": "npx expo start",
    "build-test-flight": "eas build -p ios --auto-submit",
    "build-simulator": "eas build --profile development-simulator --platform ios",
    "build-device": "eas build --profile development --platform ios",
    "build-device-standalone": "eas build --profile preview --platform ios",
    "build-production-release": "eas build --profile production --platform ios --auto-submit",
    "dev": "npx expo start --dev-client",
    "ios": "npm run ts:check && watchman watch-del-all && npx expo start --ios",
    "ios-no-ts": "watchman watch-del-all && npx expo start --ios",
    "ios-app": "npm run ts:check && watchman watch-del-all && npx expo start --dev-client",
    "ios-app-no-ts": "watchman watch-del-all && npx expo start --dev-client",
    "web": "npx expo start --web",
    "ts:check": "tsc",
    "ts": "npm run ts:check"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/react": "~18.2.45",
    "eas-cli": "^12.6.2",
    "expo": "^51.0.38",
    "expo-apple-authentication": "~6.4.2",
    "expo-blur": "~13.0.2",
    "expo-dev-client": "~4.0.28",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-linear-gradient": "~13.0.2",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.10.0",
    "i18next": "^23.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-health": "^1.19.0",
    "react-native-localize": "^3.2.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safari-view": "^2.1.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.1.0",
    "typescript": "~5.3.3",
    "uid": "^2.0.2",
    "use-count-up": "^3.0.1",
    "victory-native": "^36.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react-native": "^0.73.0",
    "@types/react-native-safari-view": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"

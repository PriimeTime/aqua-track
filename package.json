{
  "name": "Drinky",
  "version": "1.5.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "build-test-flight": "eas build -p ios --auto-submit",
    "build-simulator": "eas build --profile development-simulator --platform ios",
    "build-device": "eas build --profile development --platform ios",
    "build-device-standalone": "eas build --profile preview --platform ios",
    "build-production-release": "eas build --profile production --platform ios --auto-submit",
    "dev": "expo start --dev-client",
    "ios": "npm run ts:check && watchman watch-del-all && expo start --ios",
    "ios-no-ts": "watchman watch-del-all && expo start --ios",
    "ios-app": "npm run ts:check && watchman watch-del-all && expo start --dev-client",
    "ios-app-no-ts": "watchman watch-del-all && expo start --dev-client",
    "web": "expo start --web",
    "ts:check": "tsc",
    "ts": "npm run ts:check"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-picker/picker": "2.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/react": "~18.2.45",
    "expo": "~50.0.2",
    "expo-apple-authentication": "~6.3.0",
    "expo-blur": "~12.9.2",
    "expo-dev-client": "~3.3.8",
    "expo-font": "^11.10.2",
    "expo-haptics": "~12.8.1",
    "expo-linear-gradient": "~12.7.1",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "firebase": "^10.10.0",
    "i18next": "^23.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.73.6",
    "react-native-localize": "^3.2.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "^13.12.3",
    "react-redux": "^9.1.0",
    "typescript": "^5.3.0",
    "uid": "^2.0.2",
    "use-count-up": "^3.0.1",
    "victory-native": "^36.8.4"

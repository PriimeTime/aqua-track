{
  "expo": {
    "name": "Drinky",
    "slug": "aqua-track",
    "version": "1.6.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "ascAppId": "6478781926",
      "bundleIdentifier": "com.priimetime.aquatrack",
      "buildNumber": "1.6.8",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app uses HealthKit to track your water intake.",
        "NSHealthUpdateUsageDescription": "This app uses HealthKit to track your water intake."
      },
      "entitlements": {
        "com.apple.developer.healthkit": [
          "HKQuantityTypeIdentifierDietaryWater"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1a0f1b7c-d073-40da-b592-b79e94c02390"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Chewy-Regular.ttf"]
        }
      ],
      "expo-apple-authentication",
      "react-native-health"
    ]
  }
}

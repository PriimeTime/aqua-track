{
  "expo": {
    "name": "aqua-track",
    "slug": "aqua-track",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.priimetime.aquatrack",
      "buildNumber": "1.2.1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1a0f1b7c-d073-40da-b592-b79e94c02390"
      }
    },
    "plugins": [
      ["expo-font", { "fonts": ["./assets/fonts/Chewy-Regular.ttf"] }],
      "expo-apple-authentication"
    ]
  }
}
